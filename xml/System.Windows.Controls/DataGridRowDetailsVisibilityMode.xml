      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Windows.Controls.DataGridRowDetailsVisibilityMode">
          <source>Defines constants that specify when <ph id="ph1">&lt;see cref="T:System.Windows.Controls.DataGrid" /&gt;</ph> row details are displayed.</source>
          <target state="translated">Define constantes que especificam quando os detalhes da linha <ph id="ph1">&lt;see cref="T:System.Windows.Controls.DataGrid" /&gt;</ph> são exibidos.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Windows.Controls.DataGridRowDetailsVisibilityMode">
          <source>This enumeration is used by the <ph id="ph1">&lt;xref:System.Windows.Controls.DataGrid.RowDetailsVisibilityMode%2A?displayProperty=nameWithType&gt;</ph> property.</source>
          <target state="translated">Essa enumeração é usada pelo <ph id="ph1">&lt;xref:System.Windows.Controls.DataGrid.RowDetailsVisibilityMode%2A?displayProperty=nameWithType&gt;</ph> propriedade.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="F:System.Windows.Controls.DataGridRowDetailsVisibilityMode.Collapsed">
          <source>The row details section is not displayed for any rows.</source>
          <target state="translated">A seção de detalhes da linha não é exibida para nenhuma linha.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.Windows.Controls.DataGridRowDetailsVisibilityMode.Visible">
          <source>The row details section is displayed for all rows.</source>
          <target state="translated">A seção de detalhes da linha é exibida para todas as linhas.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.Windows.Controls.DataGridRowDetailsVisibilityMode.VisibleWhenSelected">
          <source>The row details section is displayed only for selected rows.</source>
          <target state="translated">A seção de detalhes da linha é exibida apenas para as linhas selecionadas.</target>       </trans-unit>
