      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Windows.Controls.UndoAction">
          <source>How the undo stack caused or is affected by a text change.</source>
          <target state="translated">Como a pilha de desfazer causou ou foi afetada por uma alteração de texto.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="F:System.Windows.Controls.UndoAction.Clear">
          <source>This change will clear the undo stack</source>
          <target state="translated">Essa alteração limpará a pilha de desfazer</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="F:System.Windows.Controls.UndoAction.Create">
          <source>This change will create a new undo unit</source>
          <target state="translated">Essa alteração criará uma nova unidade de desfazer</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.Windows.Controls.UndoAction.Merge">
          <source>This change will merge into the previous undo unit</source>
          <target state="translated">Essa alteração será mesclada com a unidade de desfazer anterior</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.Windows.Controls.UndoAction.None">
          <source>This change will not affect the undo stack at all</source>
          <target state="translated">Essa alteração não afetará a pilha de desfazer</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="F:System.Windows.Controls.UndoAction.Redo">
          <source>This change is the result of a call to Redo()</source>
          <target state="translated">Essa alteração é o resultado de uma chamada para Redo()</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="F:System.Windows.Controls.UndoAction.Undo">
          <source>This change is the result of a call to Undo()</source>
          <target state="translated">Essa alteração é o resultado de uma chamada para Undo()</target>       </trans-unit>
