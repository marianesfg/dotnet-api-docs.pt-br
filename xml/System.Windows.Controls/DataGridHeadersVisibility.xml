        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Windows.Controls.DataGridHeadersVisibility">
          <source>Defines constants that specify the visibility of row and column headers in a <ph id="ph1">&lt;see cref="T:System.Windows.Controls.DataGrid" /&gt;</ph>.</source>
          <target state="translated">Define constantes que especificam a visibilidade dos cabeçalhos de linhas e colunas em um <ph id="ph1">&lt;see cref="T:System.Windows.Controls.DataGrid" /&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Windows.Controls.DataGridHeadersVisibility">
          <source>This enumeration is used by the <ph id="ph1">&lt;xref:System.Windows.Controls.DataGrid.HeadersVisibility%2A&gt;</ph> property.</source>
          <target state="translated">Essa enumeração é usada pelo <ph id="ph1">&lt;xref:System.Windows.Controls.DataGrid.HeadersVisibility%2A&gt;</ph> propriedade.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Windows.Controls.DataGridHeadersVisibility">
          <source>The style of the headers is determined by the <ph id="ph1">&lt;xref:System.Windows.Controls.DataGrid.RowHeaderStyle%2A&gt;</ph> and <ph id="ph2">&lt;xref:System.Windows.Controls.DataGrid.ColumnHeaderStyle%2A&gt;</ph> properties.</source>
          <target state="translated">O estilo dos cabeçalhos é determinado pelo <ph id="ph1">&lt;xref:System.Windows.Controls.DataGrid.RowHeaderStyle%2A&gt;</ph> e <ph id="ph2">&lt;xref:System.Windows.Controls.DataGrid.ColumnHeaderStyle%2A&gt;</ph> propriedades.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.Windows.Controls.DataGridHeadersVisibility.All">
          <source>Both row and column headers are visible.</source>
          <target state="translated">Os cabeçalhos das linhas e colunas são visíveis.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.Windows.Controls.DataGridHeadersVisibility.Column">
          <source>Only column headers are visible.</source>
          <target state="translated">Somente os cabeçalhos das colunas são visíveis.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="F:System.Windows.Controls.DataGridHeadersVisibility.None">
          <source>No headers are visible.</source>
          <target state="translated">Nenhum cabeçalho é visível.</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="F:System.Windows.Controls.DataGridHeadersVisibility.Row">
