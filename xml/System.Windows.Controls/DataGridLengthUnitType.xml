    <body>
      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Windows.Controls.DataGridLengthUnitType">
          <source>Defines constants that specify how elements in a <ph id="ph1">&lt;see cref="T:System.Windows.Controls.DataGrid" /&gt;</ph> are sized.</source>
          <target state="translated">Define constantes que especificam como elementos em um <ph id="ph1">&lt;see cref="T:System.Windows.Controls.DataGrid" /&gt;</ph> são dimensionados.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="F:System.Windows.Controls.DataGridLengthUnitType.Auto">
          <source>The size is based on the contents of both the cells and the column header.</source>
          <target state="translated">O tamanho é baseado no conteúdo das células e do cabeçalho da coluna.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="F:System.Windows.Controls.DataGridLengthUnitType.Pixel">
          <source>The size is a fixed value expressed in pixels.</source>
          <target state="translated">O tamanho é um valor fixo expressado em pixels.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.Windows.Controls.DataGridLengthUnitType.SizeToCells">
          <source>The size is based on the contents of the cells.</source>
          <target state="translated">O tamanho é baseado no conteúdo das células.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.Windows.Controls.DataGridLengthUnitType.SizeToHeader">
          <source>The size is based on the contents of the column header.</source>
          <target state="translated">O tamanho é baseado no conteúdo do cabeçalho da coluna.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="F:System.Windows.Controls.DataGridLengthUnitType.Star">
          <source>The size is a weighted proportion of available space.</source>
          <target state="translated">O tamanho é uma proporção ponderada do espaço disponível.</target>       </trans-unit>
