      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Windows.Media.Imaging.DownloadProgressEventArgs">
          <source>Provides data for the <ph id="ph1">&lt;see cref="E:System.Windows.Media.Imaging.BitmapSource.DownloadProgress" /&gt;</ph> and <ph id="ph2">&lt;see cref="E:System.Windows.Media.Imaging.BitmapDecoder.DownloadProgress" /&gt;</ph> events.</source>
          <target state="translated">Fornece dados para os eventos de <ph id="ph1">&lt;see cref="E:System.Windows.Media.Imaging.BitmapSource.DownloadProgress" /&gt;</ph> e de <ph id="ph2">&lt;see cref="E:System.Windows.Media.Imaging.BitmapDecoder.DownloadProgress" /&gt;</ph> .</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="P:System.Windows.Media.Imaging.DownloadProgressEventArgs.Progress">
          <source>Gets a value that represents the download progress of a bitmap, expressed as a percentage.</source>
          <target state="translated">Obtém um valor que representa o progresso do download de um bitmap, expressado como um percentual.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Windows.Media.Imaging.DownloadProgressEventArgs.Progress">
          <source>The progress, expressed as a percentage, to which a bitmap has been downloaded.</source>
          <target state="translated">O progresso, expressado como uma porcentagem, para o qual um bitmap foi baixado.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Windows.Media.Imaging.DownloadProgressEventArgs.Progress">
          <source>The returned value will be between 1 and 100.</source>
          <target state="translated">O valor retornado será entre 1 e 100.</target>       </trans-unit>
