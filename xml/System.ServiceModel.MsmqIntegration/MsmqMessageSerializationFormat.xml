        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.ServiceModel.MsmqIntegration.MsmqMessageSerializationFormat">
          <source>Indicates the formatter that is used to serialize objects that are sent as the body of an MSMQ message.</source>
          <target state="translated">Indica o formatador que é usado para serializar objetos enviados como o corpo de uma mensagem MSMQ.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.ServiceModel.MsmqIntegration.MsmqMessageSerializationFormat">
          <source>The default value is Xml.</source>
          <target state="translated">O valor padrão é Xml.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="F:System.ServiceModel.MsmqIntegration.MsmqMessageSerializationFormat.ActiveX">
          <source>The ActiveX formatter is used to serialize the object.</source>
          <target state="translated">O formatador ActiveX é usado para serializar o objeto.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.ServiceModel.MsmqIntegration.MsmqMessageSerializationFormat.Binary">
          <source>The <ph id="ph1">&lt;see cref="T:System.Runtime.Serialization.Formatters.Binary.BinaryFormatter" /&gt;</ph> is used to serialize the object.</source>
