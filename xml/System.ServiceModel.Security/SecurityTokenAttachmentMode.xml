      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.ServiceModel.Security.SecurityTokenAttachmentMode">
          <source>An enumeration that lists the ways in which security tokens are attached.</source>
          <target state="translated">Uma enumeração que lista as maneiras em que os tokens de segurança são anexados.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.ServiceModel.Security.SecurityTokenAttachmentMode">
          <source>This enumeration specifies the signing and encrypting relationships between a message and its signature.</source>
          <target state="translated">Esta enumeração Especifica as relações de assinatura e criptografia entre uma mensagem e sua assinatura.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="F:System.ServiceModel.Security.SecurityTokenAttachmentMode.Endorsing">
          <source>The token is used to sign the message signature.</source>
          <target state="translated">O token é usado para assinar a assinatura da mensagem.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.ServiceModel.Security.SecurityTokenAttachmentMode.Signed">
          <source>The token is included in the message signature.</source>
          <target state="translated">O token é incluído na assinatura da mensagem.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.ServiceModel.Security.SecurityTokenAttachmentMode.SignedEncrypted">
          <source>The token is included in the message signature and is encrypted.</source>
          <target state="translated">O token é incluído na assinatura da mensagem e é criptografado.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="F:System.ServiceModel.Security.SecurityTokenAttachmentMode.SignedEndorsing">
          <source>The token is included in and signs the message signature.</source>
          <target state="translated">O token é incluído na assinatura da mensagem e a assina.</target>       </trans-unit>
