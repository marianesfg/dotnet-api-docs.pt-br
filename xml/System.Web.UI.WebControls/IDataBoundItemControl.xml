      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Web.UI.WebControls.IDataBoundItemControl">
          <source>Exposes the properties that are used to display a single item in a data-bound control.</source>
          <target state="translated">Expõe as propriedades usadas para exibir um único item em um controle associado a dados.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Web.UI.WebControls.IDataBoundItemControl">
          <source>This interface provides properties that are used to display a single item in the <ph id="ph1">&lt;xref:System.Web.UI.WebControls.FormView&gt;</ph> and <ph id="ph2">&lt;xref:System.Web.UI.WebControls.DetailsView&gt;</ph> data-bound controls.</source>
          <target state="translated">Essa interface fornece propriedades que são usadas para exibir um único item de <ph id="ph1">&lt;xref:System.Web.UI.WebControls.FormView&gt;</ph> e <ph id="ph2">&lt;xref:System.Web.UI.WebControls.DetailsView&gt;</ph> controles associados a dados.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="P:System.Web.UI.WebControls.IDataBoundItemControl.DataKey">
          <source>Gets the object that represents the data-key value of the row in a data-bound control.</source>
          <target state="translated">Obtém o objeto que representa o valor de chave de dados da linha em um controle associado a dados.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Web.UI.WebControls.IDataBoundItemControl.DataKey">
          <source>The object that represents the data-key value of the row in the data-bound control.</source>
          <target state="translated">O objeto que representa o valor de chave de dados da linha no controle associado a dados.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="P:System.Web.UI.WebControls.IDataBoundItemControl.Mode">
          <source>Gets the current mode of a data-bound control.</source>
          <target state="translated">Obtém o modo atual do controle associado a dados.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Web.UI.WebControls.IDataBoundItemControl.Mode">
          <source>The current mode of the data-bound control.</source>
          <target state="translated">O modo atual do controle associado a dados.</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Web.UI.WebControls.IDataBoundItemControl.Mode">
