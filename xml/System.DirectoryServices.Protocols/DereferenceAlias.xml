      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.DirectoryServices.Protocols.DereferenceAlias">
          <source>The <ph id="ph1">&lt;see cref="T:System.DirectoryServices.Protocols.DereferenceAlias" /&gt;</ph> enumeration specifies the process by which aliases are dereferenced.</source>
          <target state="translated">A enumeração <ph id="ph1">&lt;see cref="T:System.DirectoryServices.Protocols.DereferenceAlias" /&gt;</ph> especifica o processo pelo qual os aliases são desreferenciados.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="F:System.DirectoryServices.Protocols.DereferenceAlias.Always">
          <source>Dereferences aliases when both searching subordinates and locating the base object of the search.</source>
          <target state="translated">Desreferencia os aliases ao pesquisar subordinados e localizar o objeto base da pesquisa.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="F:System.DirectoryServices.Protocols.DereferenceAlias.Always">
          <source>The value is equal to 3.</source>
          <target state="translated">O valor é igual a 3.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.DirectoryServices.Protocols.DereferenceAlias.FindingBaseObject">
          <source>Dereferences aliases when locating the base object, but not when searching its subordinates.</source>
          <target state="translated">Desreferencia os aliases ao localizar o objeto base, mas não ao pesquisar seus subordinados.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.DirectoryServices.Protocols.DereferenceAlias.FindingBaseObject">
          <source>The value is equal to 2.</source>
          <target state="translated">O valor é igual a 2.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="F:System.DirectoryServices.Protocols.DereferenceAlias.InSearching">
          <source>Dereferences aliases when searching subordinates of the base object, but not when locating the base itself.</source>
          <target state="translated">Desreferencia os aliases ao pesquisar subordinados do objeto base, mas não ao localizar a base em si.</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="F:System.DirectoryServices.Protocols.DereferenceAlias.InSearching">
          <source>The value is equal to 1.</source>
          <target state="translated">O valor é igual a 1.</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="F:System.DirectoryServices.Protocols.DereferenceAlias.Never">
          <source>Does not dereference aliases.</source>
          <target state="translated">Não desreferenciar aliases.</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="F:System.DirectoryServices.Protocols.DereferenceAlias.Never">
          <source>The value is equal to 0.</source>
          <target state="translated">O valor é igual a 0.</target>       </trans-unit>
