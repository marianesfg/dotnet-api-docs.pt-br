      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Security.Authentication.ExtendedProtection.ChannelBindingKind">
          <source>The <ph id="ph1">&lt;see cref="T:System.Security.Authentication.ExtendedProtection.ChannelBindingKind" /&gt;</ph> enumeration represents the kinds of channel bindings that can be queried from secure channels.</source>
          <target state="translated">A enumeração <ph id="ph1">&lt;see cref="T:System.Security.Authentication.ExtendedProtection.ChannelBindingKind" /&gt;</ph> representa os tipos de associações de canal que podem ser consultadas de canais seguros.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="F:System.Security.Authentication.ExtendedProtection.ChannelBindingKind.Endpoint">
          <source>A channel binding unique to a given endpoint (a TLS server certificate, for example).</source>
          <target state="translated">Um canal de associação exclusivo para um determinado ponto de extremidade (um certificado do servidor TLS, por exemplo).</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="F:System.Security.Authentication.ExtendedProtection.ChannelBindingKind.Unique">
          <source>A channel binding completely unique to a given channel (a TLS session key, for example).</source>
          <target state="translated">Um canal de associação completamente exclusivo para um determinado canal (uma chave da sessão de TLS, por exemplo).</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.Security.Authentication.ExtendedProtection.ChannelBindingKind.Unknown">
          <source>An unknown channel binding type.</source>
          <target state="translated">Um tipo de associação de canal desconhecido.</target>       </trans-unit>
