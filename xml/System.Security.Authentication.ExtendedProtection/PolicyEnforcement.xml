      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Security.Authentication.ExtendedProtection.PolicyEnforcement">
          <source>The <ph id="ph1">&lt;see cref="T:System.Security.Authentication.ExtendedProtection.PolicyEnforcement" /&gt;</ph> enumeration specifies when the <ph id="ph2">&lt;see cref="T:System.Security.Authentication.ExtendedProtection.ExtendedProtectionPolicy" /&gt;</ph> should be enforced.</source>
          <target state="translated">A enumeração <ph id="ph1">&lt;see cref="T:System.Security.Authentication.ExtendedProtection.PolicyEnforcement" /&gt;</ph> especifica quando o <ph id="ph2">&lt;see cref="T:System.Security.Authentication.ExtendedProtection.ExtendedProtectionPolicy" /&gt;</ph> deve ser imposto.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="F:System.Security.Authentication.ExtendedProtection.PolicyEnforcement.Always">
          <source>The <ph id="ph1">&lt;see cref="T:System.Security.Authentication.ExtendedProtection.ExtendedProtectionPolicy" /&gt;</ph> is always enforced.</source>
          <target state="translated">O <ph id="ph1">&lt;see cref="T:System.Security.Authentication.ExtendedProtection.ExtendedProtectionPolicy" /&gt;</ph> sempre é imposto.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="F:System.Security.Authentication.ExtendedProtection.PolicyEnforcement.Always">
          <source>Clients that don’t support extended protection will fail to authenticate.</source>
          <target state="translated">Os clientes que não dão suporte à proteção estendida falharão ao autenticar.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.Security.Authentication.ExtendedProtection.PolicyEnforcement.Never">
          <source>The <ph id="ph1">&lt;see cref="T:System.Security.Authentication.ExtendedProtection.ExtendedProtectionPolicy" /&gt;</ph> is never enforced and extended protection is disabled.</source>
          <target state="translated">O <ph id="ph1">&lt;see cref="T:System.Security.Authentication.ExtendedProtection.ExtendedProtectionPolicy" /&gt;</ph> nunca é imposto e a proteção estendida está desabilitada.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.Security.Authentication.ExtendedProtection.PolicyEnforcement.WhenSupported">
          <source>The <ph id="ph1">&lt;see cref="T:System.Security.Authentication.ExtendedProtection.ExtendedProtectionPolicy" /&gt;</ph> is enforced only if the client and server supports extended protection.</source>
          <target state="translated">O <ph id="ph1">&lt;see cref="T:System.Security.Authentication.ExtendedProtection.ExtendedProtectionPolicy" /&gt;</ph> é imposto apenas se o cliente e o servidor derem suporte à proteção estendida.</target>       </trans-unit>
