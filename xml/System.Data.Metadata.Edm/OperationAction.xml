      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Data.Metadata.Edm.OperationAction">
          <source>Specifies the list of possible actions when the parent <ph id="ph1">&lt;see langword="End" /&gt;</ph> of the <ph id="ph2">&lt;see cref="T:System.Data.Metadata.Edm.RelationshipType" /&gt;</ph> is deleted.</source>
          <target state="translated">Especifica a lista de possíveis ações quando o pai <ph id="ph1">&lt;see langword="End" /&gt;</ph> do <ph id="ph2">&lt;see cref="T:System.Data.Metadata.Edm.RelationshipType" /&gt;</ph> é excluído.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="F:System.Data.Metadata.Edm.OperationAction.Cascade">
          <source>When a parent <ph id="ph1">&lt;see langword="End" /&gt;</ph> is deleted, its associated child ends are deleted as well.</source>
          <target state="translated">Quando um <ph id="ph1">&lt;see langword="End" /&gt;</ph> pai é excluído, suas extremidades filho associadas também são excluídas.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="F:System.Data.Metadata.Edm.OperationAction.None">
          <source>Only a parent <ph id="ph1">&lt;see langword="End" /&gt;</ph> can be deleted.</source>
          <target state="translated">Apenas um <ph id="ph1">&lt;see langword="End" /&gt;</ph> pai pode ser excluído.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.Data.Metadata.Edm.OperationAction.Restrict">
          <source>A parent <ph id="ph1">&lt;see langword="End" /&gt;</ph> cannot be deleted if it has one or more associated child ends.</source>
          <target state="translated">Um <ph id="ph1">&lt;see langword="End" /&gt;</ph> pai não pode ser excluído se ele tiver uma ou mais extremidades filho associadas.</target>       </trans-unit>
