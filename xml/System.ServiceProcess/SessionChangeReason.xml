      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.ServiceProcess.SessionChangeReason">
          <source>Specifies the reason for a Terminal Services session change notice.</source>
          
        </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.ServiceProcess.SessionChangeReason">
          <source>The following code example shows the use of the <ph id="ph1">&lt;xref:System.ServiceProcess.SessionChangeReason&gt;</ph> enumeration in an implementation of the <ph id="ph2">&lt;xref:System.ServiceProcess.ServiceBase.OnSessionChange%2A&gt;</ph> method in a class derived from <ph id="ph3">&lt;xref:System.ServiceProcess.ServiceBase&gt;</ph>.</source>
          <target state="translated">O exemplo de código a seguir mostra o uso do <ph id="ph1">&lt;xref:System.ServiceProcess.SessionChangeReason&gt;</ph> enumeração em uma implementação do <ph id="ph2">&lt;xref:System.ServiceProcess.ServiceBase.OnSessionChange%2A&gt;</ph> método em uma classe derivada de <ph id="ph3">&lt;xref:System.ServiceProcess.ServiceBase&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.ServiceProcess.SessionChangeReason">
          <source>This code example is part of a larger example provided for the <ph id="ph1">&lt;xref:System.ServiceProcess.ServiceBase&gt;</ph> class.</source>
          <target state="translated">Este exemplo de código é parte de um exemplo maior fornecido para a <ph id="ph1">&lt;xref:System.ServiceProcess.ServiceBase&gt;</ph> classe.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.ServiceProcess.SessionChangeReason.ConsoleConnect">
          <source>A console session has connected.</source>
          
        </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.ServiceProcess.SessionChangeReason.ConsoleDisconnect">
          <source>A console session has disconnected.</source>
          
        </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="F:System.ServiceProcess.SessionChangeReason.RemoteConnect">
          <source>A remote session has connected.</source>
          
        </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="F:System.ServiceProcess.SessionChangeReason.RemoteDisconnect">
          <source>A remote session has disconnected.</source>
          
        </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="F:System.ServiceProcess.SessionChangeReason.SessionLock">
          <source>A session has been locked.</source>
          
        </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="F:System.ServiceProcess.SessionChangeReason.SessionLogoff">
          <source>A user has logged off from a session.</source>
          
        </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" uid="F:System.ServiceProcess.SessionChangeReason.SessionLogon">
          <source>A user has logged on to a session.</source>
          
        </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" uid="F:System.ServiceProcess.SessionChangeReason.SessionRemoteControl">
          <source>The remote control status of a session has changed.</source>
          
        </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve" uid="F:System.ServiceProcess.SessionChangeReason.SessionUnlock">
          <source>A session has been unlocked.</source>
          
        </trans-unit>
