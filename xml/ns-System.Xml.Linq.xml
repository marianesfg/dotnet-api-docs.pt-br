        <trans-unit id="101" translate="yes" xml:space="preserve">
          <source>Contains the classes for LINQ to XML.</source>
          <target state="translated">Contém as classes para LINQ to XML.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve">
          <source>LINQ to XML is an in-memory XML programming interface that enables you to modify XML documents efficiently and easily.</source>
          <target state="translated">LINQ to XML é uma interface de programação XML na memória que permite modificar documentos XML de modo eficiente e fácil.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT">
          <source>Using LINQ to XML, you can:</source>
          <target state="translated">Usando o LINQ to XML, você pode:</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" extradata="MT">
          <source>Load XML from files or streams.</source>
          <target state="translated">Carregar XML de arquivos ou fluxos.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" extradata="MT">
          <source>Serialize XML to files or streams.</source>
          <target state="translated">Serializar o XML em arquivos ou fluxos.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" extradata="MT">
          <source>Create XML trees from scratch using functional construction.</source>
