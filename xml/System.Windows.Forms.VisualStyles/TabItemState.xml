        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Windows.Forms.VisualStyles.TabItemState">
          <source>Specifies the visual state of a tab item that is drawn with visual styles.</source>
          <target state="translated">Especifica o estado visual de um item de guia desenhado com estilos visuais.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Windows.Forms.VisualStyles.TabItemState">
          <source>The <ph id="ph1">&lt;xref:System.Windows.Forms.VisualStyles.TabItemState&gt;</ph> values correspond to the states of the <ph id="ph2">&lt;xref:System.Windows.Forms.VisualStyles.VisualStyleElement&gt;</ph> objects exposed by the following classes:</source>
          <target state="translated">O <ph id="ph1">&lt;xref:System.Windows.Forms.VisualStyles.TabItemState&gt;</ph> valores correspondem aos estados do <ph id="ph2">&lt;xref:System.Windows.Forms.VisualStyles.VisualStyleElement&gt;</ph> expostos pelas classes de objetos:</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Windows.Forms.VisualStyles.TabItemState">
          <source>The <ph id="ph1">&lt;xref:System.Windows.Forms.VisualStyles.TabItemState&gt;</ph> values are also used as an argument in the <ph id="ph2">&lt;xref:System.Windows.Forms.TabRenderer.DrawTabItem%2A?displayProperty=nameWithType&gt;</ph> method.</source>
          <target state="translated">O <ph id="ph1">&lt;xref:System.Windows.Forms.VisualStyles.TabItemState&gt;</ph> valores também são usados como um argumento no <ph id="ph2">&lt;xref:System.Windows.Forms.TabRenderer.DrawTabItem%2A?displayProperty=nameWithType&gt;</ph> método.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.VisualStyles.TabItemState.Disabled">
          <source>The tab item is disabled.</source>
          <target state="translated">O item de guia está desabilitado.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.VisualStyles.TabItemState.Hot">
          <source>The tab item is hot.</source>
          <target state="translated">O item de guia está ativo.</target>       </trans-unit>
