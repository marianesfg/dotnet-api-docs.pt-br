        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Windows.Forms.VisualStyles.TextBoxState">
          <source>Specifies the visual state of a text box that is drawn with visual styles.</source>
          <target state="translated">Especifica o estado visual de uma caixa de texto é desenhado com estilos visuais.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Windows.Forms.VisualStyles.TextBoxState">
          <source>The <ph id="ph1">&lt;xref:System.Windows.Forms.VisualStyles.TextBoxState&gt;</ph> values correspond to the states of the <ph id="ph2">&lt;xref:System.Windows.Forms.VisualStyles.VisualStyleElement&gt;</ph> objects exposed by the <ph id="ph3">&lt;xref:System.Windows.Forms.VisualStyles.VisualStyleElement.TextBox.TextEdit&gt;</ph> class.</source>
          <target state="translated">O <ph id="ph1">&lt;xref:System.Windows.Forms.VisualStyles.TextBoxState&gt;</ph> valores correspondem aos estados do <ph id="ph2">&lt;xref:System.Windows.Forms.VisualStyles.VisualStyleElement&gt;</ph> objetos expostos pelo <ph id="ph3">&lt;xref:System.Windows.Forms.VisualStyles.VisualStyleElement.TextBox.TextEdit&gt;</ph> classe.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Windows.Forms.VisualStyles.TextBoxState">
          <source>The <ph id="ph1">&lt;xref:System.Windows.Forms.VisualStyles.TextBoxState&gt;</ph> values are also used as an argument in the <ph id="ph2">&lt;xref:System.Windows.Forms.TextBoxRenderer.DrawTextBox%2A?displayProperty=nameWithType&gt;</ph> method.</source>
          <target state="translated">O <ph id="ph1">&lt;xref:System.Windows.Forms.VisualStyles.TextBoxState&gt;</ph> valores também são usados como um argumento no <ph id="ph2">&lt;xref:System.Windows.Forms.TextBoxRenderer.DrawTextBox%2A?displayProperty=nameWithType&gt;</ph> método.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.VisualStyles.TextBoxState.Assist">
          <source>The text box appears in assist mode.</source>
          <target state="translated">A caixa de texto aparece no modo de assistência.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.VisualStyles.TextBoxState.Disabled">
          <source>The text box appears disabled.</source>
          <target state="translated">A caixa de texto aparece desabilitada.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.VisualStyles.TextBoxState.Hot">
          <source>The text box appears hot.</source>
          <target state="translated">A caixa de texto aparece ativa.</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.VisualStyles.TextBoxState.Normal">
          <source>The text box appears normal.</source>
          <target state="translated">A caixa de texto aparece normal.</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.VisualStyles.TextBoxState.Readonly">
          <source>The text box appears read-only.</source>
          <target state="translated">Será exibida a caixa de texto somente leitura.</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.VisualStyles.TextBoxState.Selected">
          <source>The text box appears selected.</source>
          <target state="translated">A caixa de texto aparece selecionada.</target>       </trans-unit>
