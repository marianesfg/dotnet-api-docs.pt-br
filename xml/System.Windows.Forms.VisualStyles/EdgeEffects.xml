        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Windows.Forms.VisualStyles.EdgeEffects">
          <source>Specifies the visual effects that can be applied to the edges of a visual style element.</source>
          <target state="translated">Especifica os efeitos visuais que podem ser aplicados às bordas de um elemento de estilo visual.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Windows.Forms.VisualStyles.EdgeEffects">
          <source>The <ph id="ph1">&lt;xref:System.Windows.Forms.VisualStyles.EdgeEffects&gt;</ph> values are used as argument values in the <ph id="ph2">&lt;xref:System.Windows.Forms.VisualStyles.VisualStyleRenderer.DrawEdge%2A?displayProperty=nameWithType&gt;</ph> method.</source>
          <target state="translated">O <ph id="ph1">&lt;xref:System.Windows.Forms.VisualStyles.EdgeEffects&gt;</ph> valores são usados como valores de argumento no <ph id="ph2">&lt;xref:System.Windows.Forms.VisualStyles.VisualStyleRenderer.DrawEdge%2A?displayProperty=nameWithType&gt;</ph> método.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.VisualStyles.EdgeEffects.FillInterior">
          <source>The area within the element borders is filled.</source>
          <target state="translated">A área dentro das bordas do elemento é preenchida.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.VisualStyles.EdgeEffects.Flat">
          <source>The border is flat.</source>
          <target state="translated">A borda é simples.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.VisualStyles.EdgeEffects.Mono">
          <source>The border is one-dimensional.</source>
          <target state="translated">A borda é unidimensional.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.VisualStyles.EdgeEffects.None">
          <source>The border is drawn without any effects.</source>
          <target state="translated">A borda é desenhada sem efeitos.</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.VisualStyles.EdgeEffects.Soft">
          <source>The border is soft.</source>
          <target state="translated">A borda é suave.</target>       </trans-unit>
