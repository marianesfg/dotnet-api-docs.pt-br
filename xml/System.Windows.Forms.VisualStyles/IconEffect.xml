        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Windows.Forms.VisualStyles.IconEffect">
          <source>Specifies the visual effect that the visual style will apply to an icon.</source>
          <target state="translated">Especifica o efeito visual que o estilo visual aplicará a um ícone.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Windows.Forms.VisualStyles.IconEffect">
          <source>The <ph id="ph1">&lt;xref:System.Windows.Forms.VisualStyles.IconEffect&gt;</ph> values represent the return values of the <ph id="ph2">&lt;xref:System.Windows.Forms.VisualStyles.VisualStyleRenderer.GetEnumValue%2A?displayProperty=nameWithType&gt;</ph> method when it is called with an argument value of <ph id="ph3">&lt;xref:System.Windows.Forms.VisualStyles.EnumProperty.IconEffect?displayProperty=nameWithType&gt;</ph>.</source>
          <target state="translated">O <ph id="ph1">&lt;xref:System.Windows.Forms.VisualStyles.IconEffect&gt;</ph> valores representam os valores de retorno de <ph id="ph2">&lt;xref:System.Windows.Forms.VisualStyles.VisualStyleRenderer.GetEnumValue%2A?displayProperty=nameWithType&gt;</ph> método quando ele é chamado com um valor de argumento de <ph id="ph3">&lt;xref:System.Windows.Forms.VisualStyles.EnumProperty.IconEffect?displayProperty=nameWithType&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.VisualStyles.IconEffect.Alpha">
          <source>The icon is alpha-blended.</source>
          <target state="translated">O ícone passou por combinação alfa.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.VisualStyles.IconEffect.Glow">
