        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Windows.Forms.VisualStyles.PushButtonState">
          <source>Specifies the visual state of a button that is drawn with visual styles.</source>
          <target state="translated">Especifica o estado visual de um botão desenhado com estilos visuais.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Windows.Forms.VisualStyles.PushButtonState">
          <source>The <ph id="ph1">&lt;xref:System.Windows.Forms.VisualStyles.PushButtonState&gt;</ph> values correspond to the states of the <ph id="ph2">&lt;xref:System.Windows.Forms.VisualStyles.VisualStyleElement&gt;</ph> objects exposed by the <ph id="ph3">&lt;xref:System.Windows.Forms.VisualStyles.VisualStyleElement.Button.PushButton&gt;</ph> class.</source>
          <target state="translated">O <ph id="ph1">&lt;xref:System.Windows.Forms.VisualStyles.PushButtonState&gt;</ph> valores correspondem aos estados do <ph id="ph2">&lt;xref:System.Windows.Forms.VisualStyles.VisualStyleElement&gt;</ph> objetos expostos pelo <ph id="ph3">&lt;xref:System.Windows.Forms.VisualStyles.VisualStyleElement.Button.PushButton&gt;</ph> classe.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Windows.Forms.VisualStyles.PushButtonState">
          <source>The <ph id="ph1">&lt;xref:System.Windows.Forms.VisualStyles.PushButtonState&gt;</ph> values are also used as an argument in the <ph id="ph2">&lt;xref:System.Windows.Forms.ButtonRenderer.DrawButton%2A?displayProperty=nameWithType&gt;</ph> method.</source>
          <target state="translated">O <ph id="ph1">&lt;xref:System.Windows.Forms.VisualStyles.PushButtonState&gt;</ph> valores também são usados como um argumento no <ph id="ph2">&lt;xref:System.Windows.Forms.ButtonRenderer.DrawButton%2A?displayProperty=nameWithType&gt;</ph> método.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.VisualStyles.PushButtonState.Default">
          <source>The button has the default appearance.</source>
          <target state="translated">O botão tem a aparência padrão.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.VisualStyles.PushButtonState.Disabled">
          <source>The button is disabled.</source>
          <target state="translated">O botão está desabilitado.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.VisualStyles.PushButtonState.Hot">
          <source>The button is hot.</source>
          <target state="translated">O botão está ativo.</target>       </trans-unit>
