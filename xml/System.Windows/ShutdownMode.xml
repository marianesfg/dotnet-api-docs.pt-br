      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Windows.ShutdownMode">
          <source>Specifies how an application will shutdown.</source>
          <target state="translated">Especifica como um aplicativo será desligado.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="T:System.Windows.ShutdownMode">
          <source>Used by the <ph id="ph1">&lt;see cref="P:System.Windows.Application.ShutdownMode" /&gt;</ph> property.</source>
          <target state="translated">Usado pela propriedade <ph id="ph1">&lt;see cref="P:System.Windows.Application.ShutdownMode" /&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="F:System.Windows.ShutdownMode.OnExplicitShutdown">
          <source>An application shuts down only when <ph id="ph1">&lt;see cref="M:System.Windows.Application.Shutdown" /&gt;</ph> is called.</source>
          <target state="translated">Um aplicativo é desligado somente quando <ph id="ph1">&lt;see cref="M:System.Windows.Application.Shutdown" /&gt;</ph> é chamado.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.Windows.ShutdownMode.OnLastWindowClose">
          <source>An application shuts down when either the last window closes, or <ph id="ph1">&lt;see cref="M:System.Windows.Application.Shutdown" /&gt;</ph> is called.</source>
          <target state="translated">Um aplicativo é desligado quando a última janela é fechada ou <ph id="ph1">&lt;see cref="M:System.Windows.Application.Shutdown" /&gt;</ph> é chamado.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.Windows.ShutdownMode.OnMainWindowClose">
          <source>An application shuts down when either the main window closes, or <ph id="ph1">&lt;see cref="M:System.Windows.Application.Shutdown" /&gt;</ph> is called.</source>
          <target state="translated">Um aplicativo é desligado quando a janela principal é fechada ou <ph id="ph1">&lt;see cref="M:System.Windows.Application.Shutdown" /&gt;</ph> é chamado.</target>       </trans-unit>
