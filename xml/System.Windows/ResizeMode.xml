        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Windows.ResizeMode">
          <source>Specifies whether a window can be resized and, if so, how it can be resized.</source>
          <target state="translated">Especifica se uma janela pode ser redimensionada e, caso possa, como.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="T:System.Windows.ResizeMode">
          <source>Used by the <ph id="ph1">&lt;see cref="P:System.Windows.Window.ResizeMode" /&gt;</ph> property.</source>
          <target state="translated">Usado pela propriedade <ph id="ph1">&lt;see cref="P:System.Windows.Window.ResizeMode" /&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="F:System.Windows.ResizeMode.CanMinimize">
          <source>A window can only be minimized and restored.</source>
          <target state="translated">Uma janela pode ser apenas minimizada e restaurada.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.Windows.ResizeMode.CanMinimize">
          <source>The <bpt id="p1">**</bpt>Minimize<ept id="p1">**</ept> and <bpt id="p2">**</bpt>Maximize<ept id="p2">**</ept> buttons are both shown, but only the <bpt id="p3">**</bpt>Minimize<ept id="p3">**</ept> button is enabled.</source>
          <target state="translated">Os botões <bpt id="p1">**</bpt>Minimizar<ept id="p1">**</ept> e <bpt id="p2">**</bpt>Maximizar<ept id="p2">**</ept> são mostrados, mas apenas o botão <bpt id="p3">**</bpt>Minimizar<ept id="p3">**</ept> está habilitado.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.Windows.ResizeMode.CanResize">
          <source>A window can be resized.</source>
          <target state="translated">É possível redimensionar uma janela.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="F:System.Windows.ResizeMode.CanResize">
          <source>The <bpt id="p1">**</bpt>Minimize<ept id="p1">**</ept> and <bpt id="p2">**</bpt>Maximize<ept id="p2">**</ept> buttons are both shown and enabled.</source>
          <target state="translated">Os botões <bpt id="p1">**</bpt>Minimizar<ept id="p1">**</ept> e <bpt id="p2">**</bpt>Maximizar<ept id="p2">**</ept> são mostrados e habilitados.</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="F:System.Windows.ResizeMode.CanResizeWithGrip">
          <source>A window can be resized.</source>
          <target state="translated">É possível redimensionar uma janela.</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="F:System.Windows.ResizeMode.CanResizeWithGrip">
          <source>The <bpt id="p1">**</bpt>Minimize<ept id="p1">**</ept> and <bpt id="p2">**</bpt>Maximize<ept id="p2">**</ept> buttons are both shown and enabled.</source>
          <target state="translated">Os botões <bpt id="p1">**</bpt>Minimizar<ept id="p1">**</ept> e <bpt id="p2">**</bpt>Maximizar<ept id="p2">**</ept> são mostrados e habilitados.</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="F:System.Windows.ResizeMode.CanResizeWithGrip">
          <source>A resize grip appears in the bottom-right corner of the window.</source>
          <target state="translated">Uma alça de redimensionamento é exibida no canto inferior direito da janela.</target>       </trans-unit>
