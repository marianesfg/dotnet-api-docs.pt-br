      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Windows.Input.MouseButton">
          <source>Defines values that specify the buttons on a mouse device.</source>
          <target state="translated">Define os valores que especificam os botões em um dispositivo de mouse.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Windows.Input.MouseButton">
          <source>The following example creates a <ph id="ph1">&lt;xref:System.Windows.Input.Mouse.MouseDown&gt;</ph> event handler that uses the <ph id="ph2">&lt;xref:System.Windows.Input.MouseButton&gt;</ph> enumeration to determine which button was pressed.</source>
          <target state="translated">O exemplo a seguir cria um <ph id="ph1">&lt;xref:System.Windows.Input.Mouse.MouseDown&gt;</ph> manipulador de eventos que usa o <ph id="ph2">&lt;xref:System.Windows.Input.MouseButton&gt;</ph> enumeração para determinar qual botão foi pressionado.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Windows.Input.MouseButton">
          <source>Depending on which button was pressed, the background of the control that fired the event is changed.</source>
          <target state="translated">Dependendo de qual botão foi pressionado, o plano de fundo do controle que disparou o evento é alterado.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.Windows.Input.MouseButton.Left">
          <source>The left mouse button.</source>
          <target state="translated">O botão esquerdo do mouse.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.Windows.Input.MouseButton.Middle">
          <source>The middle mouse button.</source>
          <target state="translated">O botão do meio do mouse.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="F:System.Windows.Input.MouseButton.Right">
          <source>The right mouse button.</source>
          <target state="translated">O botão direito do mouse.</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="F:System.Windows.Input.MouseButton.XButton1">
          <source>The first extended mouse button.</source>
          <target state="translated">O primeiro botão do mouse estendido.</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="F:System.Windows.Input.MouseButton.XButton2">
          <source>The second extended mouse button.</source>
          <target state="translated">O segundo botão do mouse estendido.</target>       </trans-unit>
