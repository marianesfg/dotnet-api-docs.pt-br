      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Windows.Forms.BatteryChargeStatus">
          <source>Defines identifiers that indicate the current battery charge level or charging state information.</source>
          <target state="translated">Define os identificadores que fornecem informações sobre o estado de carregamento ou indicam o nível de carga da bateria atual.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Windows.Forms.BatteryChargeStatus">
          <source>This enumeration is used by the <ph id="ph1">&lt;xref:System.Windows.Forms.PowerStatus.BatteryChargeStatus%2A&gt;</ph> property of the <ph id="ph2">&lt;xref:System.Windows.Forms.PowerStatus&gt;</ph> class.</source>
          <target state="translated">Essa enumeração é usada pelo <ph id="ph1">&lt;xref:System.Windows.Forms.PowerStatus.BatteryChargeStatus%2A&gt;</ph> propriedade o <ph id="ph2">&lt;xref:System.Windows.Forms.PowerStatus&gt;</ph> classe.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.BatteryChargeStatus.Charging">
          <source>Indicates a battery is charging.</source>
          <target state="translated">Indica que uma bateria está sendo carregada.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.BatteryChargeStatus.Critical">
          <source>Indicates a critically low level of battery charge.</source>
          <target state="translated">Indica um nível criticamente baixo de carga da bateria.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.BatteryChargeStatus.High">
          <source>Indicates a high level of battery charge.</source>
          <target state="translated">Indica um alto nível de carga da bateria.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.BatteryChargeStatus.Low">
          <source>Indicates a low level of battery charge.</source>
          <target state="translated">Indica um baixo nível de carga da bateria.</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.BatteryChargeStatus.NoSystemBattery">
          <source>Indicates that no battery is present.</source>
          <target state="translated">Indica que nenhuma bateria está presente.</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.BatteryChargeStatus.Unknown">
          <source>Indicates an unknown battery condition.</source>
          <target state="translated">Indica uma condição da bateria desconhecida.</target>       </trans-unit>
