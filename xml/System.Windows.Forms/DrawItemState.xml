      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Windows.Forms.DrawItemState">
          <source>Specifies the state of an item that is being drawn.</source>
          <target state="translated">Especifica o estado de um item que está sendo desenhado.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Windows.Forms.DrawItemState">
          <source>This enumeration is used by members such as <ph id="ph1">&lt;xref:System.Windows.Forms.DrawItemEventArgs.State%2A?displayProperty=nameWithType&gt;</ph>.</source>
          <target state="translated">Essa enumeração é usada por membros como <ph id="ph1">&lt;xref:System.Windows.Forms.DrawItemEventArgs.State%2A?displayProperty=nameWithType&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Windows.Forms.DrawItemState">
          <source>The following code example demonstrates using the <ph id="ph1">&lt;xref:System.Drawing.Graphics&gt;</ph> property to perform custom drawing of the items in a <ph id="ph2">&lt;xref:System.Windows.Forms.ListBox&gt;</ph>.</source>
          <target state="translated">O exemplo de código a seguir demonstra como usar o <ph id="ph1">&lt;xref:System.Drawing.Graphics&gt;</ph> propriedade executar desenho padrão dos itens em um <ph id="ph2">&lt;xref:System.Windows.Forms.ListBox&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.DrawItemState.Checked">
          <source>The item is checked.</source>
          <target state="translated">O item é verificado.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.DrawItemState.Checked">
          <source>Only menu controls use this value.</source>
          <target state="translated">Somente os controles menu usam esse valor.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.DrawItemState.ComboBoxEdit">
          <source>The item is the editing portion of a <ph id="ph1">&lt;see cref="T:System.Windows.Forms.ComboBox" /&gt;</ph>.</source>
          <target state="translated">O item é a parte de edição de um <ph id="ph1">&lt;see cref="T:System.Windows.Forms.ComboBox" /&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.DrawItemState.Default">
          <source>The item is in its default visual state.</source>
          <target state="translated">O item está em seu estado visual padrão.</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.DrawItemState.Disabled">
          <source>The item is unavailable.</source>
          <target state="translated">O item não está disponível.</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.DrawItemState.Focus">
          <source>The item has focus.</source>
          <target state="translated">O item tem o foco.</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.DrawItemState.Grayed">
          <source>The item is grayed.</source>
          <target state="translated">O item está esmaecido.</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.DrawItemState.Grayed">
          <source>Only menu controls use this value.</source>
          <target state="translated">Somente os controles menu usam esse valor.</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.DrawItemState.HotLight">
          <source>The item is being hot-tracked, that is, the item is highlighted as the mouse pointer passes over it.</source>
          <target state="translated">O item está utilizando o rastreio importante, isto é, o item é realçado conforme o ponteiro do mouse passa sobre ele.</target>       </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.DrawItemState.Inactive">
          <source>The item is inactive.</source>
          <target state="translated">O item está inativo.</target>       </trans-unit>
        <trans-unit id="114" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.DrawItemState.NoAccelerator">
          <source>The item displays without a keyboard accelerator.</source>
          <target state="translated">O item é exibido sem um acelerador de teclado.</target>       </trans-unit>
        <trans-unit id="115" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.DrawItemState.NoFocusRect">
          <source>The item displays without the visual cue that indicates it has focus.</source>
          <target state="translated">O item é exibido sem a indicação visual que indica que ele tem o foco.</target>       </trans-unit>
        <trans-unit id="116" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.DrawItemState.None">
          <source>The item currently has no state.</source>
          <target state="translated">O item atualmente não tem estado.</target>       </trans-unit>
        <trans-unit id="117" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.DrawItemState.Selected">
          <source>The item is selected.</source>
          <target state="translated">O item está selecionado.</target>       </trans-unit>
