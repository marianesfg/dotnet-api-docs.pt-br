      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Web.UI.DataVisualization.Charting.AntiAliasingStyles">
          <source>Specifies the anti-aliasing options for the chart.</source>
          <target state="translated">Especifica as opções de suavização para o gráfico.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="F:System.Web.UI.DataVisualization.Charting.AntiAliasingStyles.All">
          <source>Use anti-aliasing for text and graphics.</source>
          <target state="translated">Use a suavização para texto e elementos gráficos.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="F:System.Web.UI.DataVisualization.Charting.AntiAliasingStyles.Graphics">
          <source>Use anti-aliasing when drawing graphics primitives such as circles or rectangles.</source>
          <target state="translated">Use a suavização ao desenhar primitivas gráficas, como círculos ou retângulos.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.Web.UI.DataVisualization.Charting.AntiAliasingStyles.None">
          <source>Do not use anti-aliasing.</source>
          <target state="translated">Não use a suavização.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.Web.UI.DataVisualization.Charting.AntiAliasingStyles.Text">
          <source>Use anti-aliasing when drawing text.</source>
          <target state="translated">Use a suavização ao desenhar texto.</target>       </trans-unit>
