      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Web.UI.DataVisualization.Charting.GridTickTypes">
          <source>Specifies the options for displaying a grid line or a tick mark for a custom label.</source>
          <target state="translated">Especifica as opções para exibir uma linha de grade ou uma marca de escala para um rótulo personalizado.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Web.UI.DataVisualization.Charting.GridTickTypes">
          <source>The <ph id="ph1">&lt;xref:System.Web.UI.DataVisualization.Charting.GridTickTypes&gt;</ph> enumeration is used in conjunction with the <ph id="ph2">&lt;xref:System.Web.UI.DataVisualization.Charting.CustomLabel.GridTicks%2A&gt;</ph> property to display a grid line and/or a tick mark for a custom label.</source>
          <target state="translated">O <ph id="ph1">&lt;xref:System.Web.UI.DataVisualization.Charting.GridTickTypes&gt;</ph> enumeração é usada em conjunto com o <ph id="ph2">&lt;xref:System.Web.UI.DataVisualization.Charting.CustomLabel.GridTicks%2A&gt;</ph> marca de propriedade para exibir uma linha de grade e/ou em uma escala para um rótulo personalizado.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="F:System.Web.UI.DataVisualization.Charting.GridTickTypes.All">
          <source>A tick mark and a grid line are shown.</source>
          <target state="translated">Uma marca de escala e uma linha de grade são mostradas.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.Web.UI.DataVisualization.Charting.GridTickTypes.Gridline">
          <source>A grid line is shown.</source>
          <target state="translated">Uma linha de grade é mostrada.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.Web.UI.DataVisualization.Charting.GridTickTypes.None">
          <source>No tick mark or grid line is shown.</source>
          <target state="translated">Nenhuma linha de grade ou marca de escala é mostrada.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="F:System.Web.UI.DataVisualization.Charting.GridTickTypes.TickMark">
          <source>A tick mark is shown.</source>
          <target state="translated">Uma marca de escala é mostrada.</target>       </trans-unit>
