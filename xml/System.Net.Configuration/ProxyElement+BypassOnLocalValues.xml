      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Net.Configuration.ProxyElement.BypassOnLocalValues">
          <source>Specifies whether the proxy is bypassed for local resources.</source>
          <target state="translated">Especifica se o proxy é ignorado para os recursos locais.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Net.Configuration.ProxyElement.BypassOnLocalValues">
          <source>Local resources include the local server (<ph id="ph1">http://localhost</ph>, <ph id="ph2">http://loopback</ph>, or <ph id="ph3">http://127.0.0.1)</ph> and a URI without a period (<ph id="ph4">http://webserver)</ph>.</source>
          <target state="translated">Recursos locais incluem o servidor local (<ph id="ph1">http://localhost</ph>, <ph id="ph2">http://loopback</ph>, ou <ph id="ph3">http://127.0.0.1)</ph> e um URI sem um período (<ph id="ph4">http://webserver)</ph>.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="F:System.Net.Configuration.ProxyElement.BypassOnLocalValues.False">
          <source>All requests for local resources should go through the proxy</source>
          <target state="translated">Todas as solicitações de recursos locais devem percorrer o proxy</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.Net.Configuration.ProxyElement.BypassOnLocalValues.True">
          <source>Access local resources directly.</source>
          <target state="translated">Acesse os recursos locais diretamente.</target>       </trans-unit>
