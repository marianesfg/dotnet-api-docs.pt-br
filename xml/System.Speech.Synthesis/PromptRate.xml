      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Speech.Synthesis.PromptRate">
          <source>Enumerates values for the speaking rate of prompts.</source>
          <target state="translated">Enumera os valores para a taxa de fala de avisos.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Speech.Synthesis.PromptRate">
          <source>Members of the <ph id="ph1">&lt;xref:System.Speech.Synthesis.PromptRate&gt;</ph> enumeration are used by the <ph id="ph2">&lt;xref:System.Speech.Synthesis.PromptStyle.%23ctor%2A&gt;</ph> constructor, by the <ph id="ph3">&lt;xref:System.Speech.Synthesis.PromptBuilder.AppendText%2A&gt;</ph> method, and by the <ph id="ph4">&lt;xref:System.Speech.Synthesis.PromptBuilder.StartStyle%2A&gt;</ph> method to specify the rate of speech for spoken text.</source>
          <target state="translated">Membros do <ph id="ph1">&lt;xref:System.Speech.Synthesis.PromptRate&gt;</ph> enumeração são usados pelo <ph id="ph2">&lt;xref:System.Speech.Synthesis.PromptStyle.%23ctor%2A&gt;</ph> construtor, pelo <ph id="ph3">&lt;xref:System.Speech.Synthesis.PromptBuilder.AppendText%2A&gt;</ph> método e o <ph id="ph4">&lt;xref:System.Speech.Synthesis.PromptBuilder.StartStyle%2A&gt;</ph> método para especificar a taxa de fala para texto falado.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Speech.Synthesis.PromptRate">
          <source>The <ph id="ph1">&lt;xref:System.Speech.Synthesis.PromptStyle.Rate%2A&gt;</ph> property gets the rate for a <ph id="ph2">&lt;xref:System.Speech.Synthesis.PromptStyle&gt;</ph> object using a <ph id="ph3">&lt;xref:System.Speech.Synthesis.PromptRate&gt;</ph> instance.</source>
          <target state="translated">O <ph id="ph1">&lt;xref:System.Speech.Synthesis.PromptStyle.Rate%2A&gt;</ph> propriedade obtém a taxa de um <ph id="ph2">&lt;xref:System.Speech.Synthesis.PromptStyle&gt;</ph> objeto usando um <ph id="ph3">&lt;xref:System.Speech.Synthesis.PromptRate&gt;</ph> instância.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.Speech.Synthesis.PromptRate.ExtraFast">
          <source>Indicates an extra-fast rate.</source>
          <target state="translated">Indica uma taxa extremamente rápida.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.Speech.Synthesis.PromptRate.ExtraSlow">
          <source>Indicates an extra-slow rate.</source>
          <target state="translated">Indica uma taxa extremamente lenta.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="F:System.Speech.Synthesis.PromptRate.Fast">
          <source>Indicates a fast rate.</source>
          <target state="translated">Indica uma taxa rápida.</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="F:System.Speech.Synthesis.PromptRate.Medium">
          <source>Indicates a medium rate.</source>
          <target state="translated">Indica uma taxa média.</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="F:System.Speech.Synthesis.PromptRate.NotSet">
          <source>Indicates no rate is specified.</source>
          <target state="translated">Indica que nenhuma taxa foi especificada.</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="F:System.Speech.Synthesis.PromptRate.Slow">
          <source>Indicates a slow rate.</source>
          <target state="translated">Indica uma taxa lenta.</target>       </trans-unit>
