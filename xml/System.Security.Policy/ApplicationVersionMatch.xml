        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Security.Policy.ApplicationVersionMatch">
          <source>Specifies how to match versions when locating application trusts in a collection.</source>
          <target state="translated">Especifica como fazer a correspondência de versões quando localizar relações de confiança de aplicativo em uma coleção.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Security.Policy.ApplicationVersionMatch">
          <source>The <ph id="ph1">&lt;xref:System.Security.Policy.ApplicationTrustCollection.Remove%2A&gt;</ph> and <ph id="ph2">&lt;xref:System.Security.Policy.ApplicationTrustCollection.Find%2A&gt;</ph> methods of an <ph id="ph3">&lt;xref:System.Security.Policy.ApplicationTrustCollection&gt;</ph> can act either on all versions of an application or on the exact version.</source>
          <target state="translated">O <ph id="ph1">&lt;xref:System.Security.Policy.ApplicationTrustCollection.Remove%2A&gt;</ph> e <ph id="ph2">&lt;xref:System.Security.Policy.ApplicationTrustCollection.Find%2A&gt;</ph> métodos de um <ph id="ph3">&lt;xref:System.Security.Policy.ApplicationTrustCollection&gt;</ph> pode agir em todas as versões de um aplicativo ou a versão exata.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Security.Policy.ApplicationVersionMatch">
          <source>The <ph id="ph1">&lt;xref:System.Security.Policy.ApplicationVersionMatch&gt;</ph> enumeration allows the caller to specify the desired behavior.</source>
          <target state="translated">O <ph id="ph1">&lt;xref:System.Security.Policy.ApplicationVersionMatch&gt;</ph> enumeração permite que o chamador especificar o comportamento desejado.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.Security.Policy.ApplicationVersionMatch.MatchAllVersions">
          <source>Match on all versions.</source>
          <target state="translated">Corresponder em todas as versões.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.Security.Policy.ApplicationVersionMatch.MatchExactVersion">
          <source>Match on the exact version.</source>
          <target state="translated">Corresponder na versão exata.</target>       </trans-unit>
