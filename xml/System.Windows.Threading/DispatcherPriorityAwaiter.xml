      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Windows.Threading.DispatcherPriorityAwaiter">
          <source>Represents an object that waits for the completion of an asynchronous task.</source>
          <target state="translated">Representa um objeto que aguarda a conclusão de uma tarefa assíncrona.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="M:System.Windows.Threading.DispatcherPriorityAwaiter.GetResult">
          <source>Ends the wait for the completion of the asynchronous task.</source>
          <target state="translated">Termina a espera pela conclusão da tarefa assíncrona.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="P:System.Windows.Threading.DispatcherPriorityAwaiter.IsCompleted">
          <source>Gets a value that indicates whether the asynchronous task has completed.</source>
          <target state="translated">Obtém um valor que indica se a tarefa assíncrona foi concluída.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Windows.Threading.DispatcherPriorityAwaiter.IsCompleted">
          <source><ph id="ph1">&lt;see langword="false" /&gt;</ph> in all cases.</source>
          <target state="translated"><ph id="ph1">&lt;see langword="false" /&gt;</ph> em todos os casos.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="M:System.Windows.Threading.DispatcherPriorityAwaiter.OnCompleted(System.Action)">
          <source>The action to perform when the wait operation completes.</source>
          <target state="translated">A ação a ser executada quando a operação de espera é concluída.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="M:System.Windows.Threading.DispatcherPriorityAwaiter.OnCompleted(System.Action)">
          <source>Sets the action to perform when the <ph id="ph1">&lt;see cref="T:System.Windows.Threading.DispatcherPriorityAwaiter" /&gt;</ph> object stops waiting for the asynchronous task to complete.</source>
