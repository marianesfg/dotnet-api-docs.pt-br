      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.ServiceModel.NetNamedPipeSecurityMode">
          <source>Specifies whether transport-level security is used with named pipes.</source>
          <target state="translated">Especifica se a segurança em nível de transporte é usada com pipes nomeados.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.ServiceModel.NetNamedPipeSecurityMode">
          <source>Use this enumeration to specify whether transport-level security is used with named pipes when using the <ph id="ph1">&lt;xref:System.ServiceModel.NetNamedPipeBinding&gt;</ph>.</source>
          <target state="translated">Use esta enumeração para especificar se a segurança em nível de transporte é usada com pipes nomeados, ao usar o <ph id="ph1">&lt;xref:System.ServiceModel.NetNamedPipeBinding&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.ServiceModel.NetNamedPipeSecurityMode">
          <source>The default value is Transport.</source>
          <target state="translated">O valor padrão é o transporte.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.ServiceModel.NetNamedPipeSecurityMode.None">
          <source>Indicates no security is used with the named pipe transport.</source>
          <target state="translated">Indica que nenhuma segurança é usada com o transporte de pipe nomeado.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.ServiceModel.NetNamedPipeSecurityMode.Transport">
          <source>Indicates that transport-level security is used with the named pipe transport.</source>
          <target state="translated">Indica que a segurança de nível de transporte é usada com o transporte de pipe nomeado.</target>       </trans-unit>
