        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Windows.Controls.Primitives.ItemsChangedEventArgs">
          <source>Provides data for the <ph id="ph1">&lt;see cref="E:System.Windows.Controls.ItemContainerGenerator.ItemsChanged" /&gt;</ph> event.</source>
          <target state="translated">Fornece dados para o evento de <ph id="ph1">&lt;see cref="E:System.Windows.Controls.ItemContainerGenerator.ItemsChanged" /&gt;</ph> .</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Windows.Controls.Primitives.ItemsChangedEventArgs">
          <source>The <ph id="ph1">&lt;xref:System.Windows.Controls.ItemContainerGenerator.ItemsChanged&gt;</ph> event is raised by an <ph id="ph2">&lt;xref:System.Windows.Controls.Primitives.IItemContainerGenerator&gt;</ph> to inform layouts that the items collection has changed.</source>
          <target state="translated">O <ph id="ph1">&lt;xref:System.Windows.Controls.ItemContainerGenerator.ItemsChanged&gt;</ph> é gerado por um <ph id="ph2">&lt;xref:System.Windows.Controls.Primitives.IItemContainerGenerator&gt;</ph> para informar os layouts de que a coleção de itens foi alterado.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="P:System.Windows.Controls.Primitives.ItemsChangedEventArgs.Action">
          <source>Gets the action that occurred on the items collection.</source>
          <target state="translated">Obtém a ação que ocorreu na coleção de itens.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Windows.Controls.Primitives.ItemsChangedEventArgs.Action">
