      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Windows.Forms.DataVisualization.Charting.ScrollBarButtonStyles">
          <source>Specifies the type of button that will be displayed on the scrollbar.</source>
          <target state="translated">Especifica o tipo de botão que será exibido na barra de rolagem.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Windows.Forms.DataVisualization.Charting.ScrollBarButtonStyles">
          <source>The <ph id="ph1">&lt;xref:System.Windows.Forms.DataVisualization.Charting.ScrollBarButtonStyles&gt;</ph> enumeration is used with the <ph id="ph2">&lt;xref:System.Windows.Forms.DataVisualization.Charting.AxisScrollBar.ButtonStyle%2A&gt;</ph> property to specify the buttons to be displayed for a scrollbar.</source>
          <target state="translated">O <ph id="ph1">&lt;xref:System.Windows.Forms.DataVisualization.Charting.ScrollBarButtonStyles&gt;</ph> enumeração é usada com a <ph id="ph2">&lt;xref:System.Windows.Forms.DataVisualization.Charting.AxisScrollBar.ButtonStyle%2A&gt;</ph> propriedade para especificar os botões a serem exibidos para uma barra de rolagem.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.DataVisualization.Charting.ScrollBarButtonStyles.All">
          <source>All buttons are displayed.</source>
          <target state="translated">Todos os botões são exibidos.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.DataVisualization.Charting.ScrollBarButtonStyles.None">
          <source>No buttons are displayed.</source>
          <target state="translated">Nenhum botão é exibido.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.DataVisualization.Charting.ScrollBarButtonStyles.ResetZoom">
          <source>The reset and zoom (thumb) buttons are displayed.</source>
          <target state="translated">Os botões de redefinição e de zoom (miniatura) são exibidos.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.DataVisualization.Charting.ScrollBarButtonStyles.SmallScroll">
          <source>The up/down and left/right buttons only are displayed.</source>
          <target state="translated">Apenas os botões cima/baixo e esquerda/direita são exibidos.</target>       </trans-unit>
