        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Windows.Forms.DataVisualization.Charting.ChartImageFormat">
          <source>Specifies the image type of the chart.</source>
          <target state="translated">Especifica o tipo de imagem do gráfico.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Windows.Forms.DataVisualization.Charting.ChartImageFormat">
          <source>The <ph id="ph1">&lt;xref:System.Windows.Forms.DataVisualization.Charting.ChartImageFormat&gt;</ph> enumeration represents an image type that is used to save the chart to a file or stream.</source>
          <target state="translated">O <ph id="ph1">&lt;xref:System.Windows.Forms.DataVisualization.Charting.ChartImageFormat&gt;</ph> enumeração representa um tipo de imagem que é usado para salvar o gráfico em um arquivo ou fluxo.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Windows.Forms.DataVisualization.Charting.ChartImageFormat">
          <source>Only charts saved to disk can use the EmfDual or EmfDual format.</source>
          <target state="translated">Somente gráficos salvos em disco podem usar o EmfDual ou EmfDual formato.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Windows.Forms.DataVisualization.Charting.ChartImageFormat">
