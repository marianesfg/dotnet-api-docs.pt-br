      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.ServiceModel.Channels.WebContentFormat">
          <source>Specifies the message formats to which content types of incoming messages can be mapped.</source>
          <target state="translated">Especifica os formatos de mensagem para que os tipos de conteúdo de mensagens de entrada possam ser mapeados.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.ServiceModel.Channels.WebContentFormat">
          <source>A value of <ph id="ph1">&lt;xref:System.ServiceModel.Channels.WebContentFormat&gt;</ph> is returned by an instance of <ph id="ph2">&lt;xref:System.ServiceModel.Channels.WebContentTypeMapper&gt;</ph> to indicate the format to which the content type of an incoming message is mapped.</source>
          <target state="translated">Um valor de <ph id="ph1">&lt;xref:System.ServiceModel.Channels.WebContentFormat&gt;</ph> é retornado por uma instância de <ph id="ph2">&lt;xref:System.ServiceModel.Channels.WebContentTypeMapper&gt;</ph> para indicar o formato que o tipo de conteúdo de uma mensagem de entrada é mapeado.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="F:System.ServiceModel.Channels.WebContentFormat.Default">
          <source>The format to map to cannot be determined.</source>
          <target state="translated">Não foi possível determinar o formato a ser mapeado.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.ServiceModel.Channels.WebContentFormat.Json">
          <source>Map to the JSON format.</source>
          <target state="translated">Mapear para o formato JSON.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.ServiceModel.Channels.WebContentFormat.Raw">
          <source>Map to the "Raw" binary format.</source>
          <target state="translated">Mapear para o formato binário "Bruto".</target>       </trans-unit>
