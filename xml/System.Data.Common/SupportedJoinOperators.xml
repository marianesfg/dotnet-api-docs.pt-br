      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Data.Common.SupportedJoinOperators">
          <source>Specifies what types of Transact-SQL join statements are supported by the data source.</source>
          <target state="translated">Especifica quais tipos de instruções de junção Transact-SQL têm suporte da fonte de dados.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Data.Common.SupportedJoinOperators">
          <source>These members are flags that allow a bitwise combination of the join capabilities.</source>
          <target state="translated">Esses membros são sinalizadores que permite uma combinação bit a bit dos recursos de junção.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="F:System.Data.Common.SupportedJoinOperators.FullOuter">
          <source>The data source supports full outer joins.</source>
          <target state="translated">A fonte de dados dá suporte a junções externas completas.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.Data.Common.SupportedJoinOperators.Inner">
          <source>The data source supports inner joins.</source>
          <target state="translated">A fonte de dados dá suporte a junções internas.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.Data.Common.SupportedJoinOperators.LeftOuter">
          <source>The data source supports left outer joins.</source>
          <target state="translated">A fonte de dados dá suporte a junções externas esquerdas.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="F:System.Data.Common.SupportedJoinOperators.None">
          <source>The data source does not support join queries.</source>
          <target state="translated">A fonte de dados não oferece suporte a consultas de junção.</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="F:System.Data.Common.SupportedJoinOperators.RightOuter">
          <source>The data source supports right outer joins.</source>
          <target state="translated">A fonte de dados dá suporte a junções externas direitas.</target>       </trans-unit>
