    <Member MemberName="PreserveFullTrustPermissionSet">
      <MemberSignature Language="C#" Value="public bool PreserveFullTrustPermissionSet { get; set; }" />
      <MemberSignature Language="ILAsm" Value=".property instance bool PreserveFullTrustPermissionSet" />
      <MemberSignature Language="DocId" Value="P:Microsoft.Build.Tasks.Deployment.ManifestUtilities.TrustInfo.PreserveFullTrustPermissionSet" />
