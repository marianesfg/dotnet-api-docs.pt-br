    <body>
      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Windows.Automation.WindowInteractionState">
          <source>Contains values that specify the current state of the window for purposes of user or programmatic interaction.</source>
          <target state="translated">Contém valores que especificam o estado atual da janela para fins de interação programática ou do usuário.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="F:System.Windows.Automation.WindowInteractionState.BlockedByModalWindow">
          <source>Indicates that the window is blocked by a modal window.</source>
          <target state="translated">Indica que a janela está bloqueada por uma janela modal.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="F:System.Windows.Automation.WindowInteractionState.Closing">
          <source>Indicates that the window is closing.</source>
          <target state="translated">Indica que a janela está sendo fechada.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.Windows.Automation.WindowInteractionState.NotResponding">
          <source>Indicates that the window is not responding.</source>
          <target state="translated">Indica que a janela não está respondendo.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.Windows.Automation.WindowInteractionState.ReadyForUserInteraction">
          <source>Indicates that the window is ready for user interaction.</source>
          <target state="translated">Indica que a janela está pronta para a interação do usuário.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="F:System.Windows.Automation.WindowInteractionState.Running">
          <source>Indicates that the window is running.</source>
          <target state="translated">Indica se a janela está em execução.</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="F:System.Windows.Automation.WindowInteractionState.Running">
          <source>This does not guarantee that the window is responding or ready for user interaction.</source>
          <target state="translated">Isso não garante que a janela esteja respondendo ou esteja pronta para interação do usuário.</target>       </trans-unit>
