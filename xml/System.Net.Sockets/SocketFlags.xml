    <body>
      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Net.Sockets.SocketFlags">
          <source>Specifies socket send and receive behaviors.</source>
          <target state="translated">Especifica os comportamentos de envio e recebimento do soquete.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Net.Sockets.SocketFlags">
          <source>The following example sends data and specifies None for <ph id="ph1">&lt;xref:System.Net.Sockets.SocketFlags.None&gt;</ph>.</source>
          <target state="translated">O exemplo a seguir envia os dados e especifica que nenhum para <ph id="ph1">&lt;xref:System.Net.Sockets.SocketFlags.None&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="F:System.Net.Sockets.SocketFlags.Broadcast">
          <source>Indicates a broadcast packet.</source>
          <target state="translated">Indica um pacote de difusão.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.Net.Sockets.SocketFlags.ControlDataTruncated">
          <source>Indicates that the control data did not fit into an internal 64-KB buffer and was truncated.</source>
          <target state="translated">Indica que os dados de controle não couberam em um buffer interno de 64 KB e foram truncados.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.Net.Sockets.SocketFlags.DontRoute">
          <source>Send without using routing tables.</source>
          <target state="translated">Enviar sem usar tabelas de roteamento.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="F:System.Net.Sockets.SocketFlags.MaxIOVectorLength">
          <source>Provides a standard value for the number of WSABUF structures that are used to send and receive data.</source>
          <target state="translated">Fornece um valor padrão para o número de estruturas WSABUF que são usadas para enviar e receber dados.</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="F:System.Net.Sockets.SocketFlags.MaxIOVectorLength">
          <source>This value is not used or supported on <ph id="ph1">[!INCLUDE[net_v45](~/includes/net-v45-md.md)]</ph>.</source>
          <target state="translated">Esse valor não é usado ou tem suporte em <ph id="ph1">[!INCLUDE[net_v45](~/includes/net-v45-md.md)]</ph>.</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="F:System.Net.Sockets.SocketFlags.Multicast">
          <source>Indicates a multicast packet.</source>
          <target state="translated">Indica um pacote de multicast.</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="F:System.Net.Sockets.SocketFlags.None">
          <source>Use no flags for this call.</source>
          <target state="translated">Não usar sinalizadores para essa chamada.</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" uid="F:System.Net.Sockets.SocketFlags.OutOfBand">
          <source>Process out-of-band data.</source>
          <target state="translated">Processar dados fora de banda.</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" uid="F:System.Net.Sockets.SocketFlags.Partial">
          <source>Partial send or receive for message.</source>
          <target state="translated">Envio ou recebimento parcial da mensagem.</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve" uid="F:System.Net.Sockets.SocketFlags.Peek">
          <source>Peek at the incoming message.</source>
          <target state="translated">Inspecionar a mensagem de entrada.</target>       </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve" uid="F:System.Net.Sockets.SocketFlags.Truncated">
          <source>The message was too large to fit into the specified buffer and was truncated.</source>
          <target state="translated">A mensagem era muito grande para caber no buffer especificado e foi truncada.</target>       </trans-unit>
      </group>
